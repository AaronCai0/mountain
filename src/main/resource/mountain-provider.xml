<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xmlns:context="http://www.springframework.org/schema/context"
       xmlns:mountain="http://www.mountainframework.com/mountain"
       xsi:schemaLocation="
			http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-3.0.xsd
			http://www.springframework.org/schema/context http://www.springframework.org/schema/context/spring-context.xsd
		    http://www.mountainframework.com/mountain 
		    http://www.mountainframework.com/mountain/mountain.xsd">
		    
  <mountain:registry address="127.0.0.1:9999" />
  <mountain:provider timeout="20000"/>
  
  
  
  <mountain:service interface="com.mountainframework.client.CalcService" ref="calcService"/>
  <bean id="calcService" class="com.mountainframework.client.CalcServiceImpl" />
  
  <mountain:service interface="com.mountainframework.client.CalcService2" ref="calcService2"/>
  <bean id="calcService2" class="com.mountainframework.client.CalcServiceImpl2" />
  
</beans>